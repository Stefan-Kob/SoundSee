@model SoundSee.ViewModels.UserViewModel
@{
    ViewData["Title"] = "Search";
    Layout = "~/Views/Shared/_SignInLayout.cshtml";
}
<script>
    import { Input, initMDB } from "mdb-ui-kit";
    initMDB({ Input });
</script>

<!-- Navbar -->
<div class="mb-2 fixed-top">
    <nav class="navbar navbar-light mt-0" id="topSignedInBar">
        <div class="col-10">
            <div class="form-outline" data-mdb-input-init>
                <input type="search" id="form1" class="form-control" placeholder="Type query" aria-label="Search" />
            </div>
        </div>
        <img src="~/Media/search.svg" width="30" height="30" alt="Search">

    </nav>
</div>
